SELECT c."FirstName",c."LastName",m."MovieTitle",m."Link" AS "MovieLink", t."Name" AS Songs
FROM "Invoice" i
LEFT JOIN "InvoiceLine" il
ON i."InvoiceId" = il."InvoiceId"
LEFT JOIN "Movie" m
ON il."MovieId" = m."MovieId"
LEFT JOIN "Customer" c
ON c."CustomerId" = i."CustomerId"
LEFT JOIN "Track" t
ON t."TrackId" = il."TrackId"
WHERE c."CustomerId" = 53;